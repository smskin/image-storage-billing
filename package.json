        "datatables.net-fixedheader-bs4": "^3.1.3",
        "datatables.net-keytable-bs4": "^2.3.2",
        "datatables.net-responsive-bs4": "^2.2.1",
        "datatables.net-rowgroup-bs4": "^1.0.2",
