        "popper.js": "^1.12",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "bootstrap-sass": "^3.4.1",
        "datatables.net": "latest",
        "datatables.net-autofill-bs4": "^2.2.2",
